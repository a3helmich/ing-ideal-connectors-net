<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - iDealSample6</title>
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/iDealSample6.styles.css" asp-append-version="true" />
</head>
<body>
  <header>
    <nav class="navbar navbar-expand-sm navbar-light bg-white border-bottom box-shadow mb-3">
      <div class="container">
        <table border="0" width="100%">
          <tr>
            <td width="120">
              <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="/ideal_logo.gif" alt="" /></a>
            </td>
            <td>
              <span style="font: bold 24pt arial">Advanced Connector - .NetCore 6</span>
              <br />
              <span style="font: 18pt arial">**Test Page**</span>
            </td>
          </tr>
        </table>
      </div>
    </nav>
  </header>
  <div class="container">
    <main role="main" class="pb-3">
      @RenderBody()
    </main>
  </div>

  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>
  @RenderSection("Scripts", required: false)
</body>
</html>
