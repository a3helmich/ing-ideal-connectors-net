<Project>
	<PropertyGroup>
		<Version>2021.1.03</Version>
    
		<Authors>Adrie Helmich</Authors>
		<Copyright>Copyright Â© ING Group 2007-2017</Copyright>
		<Company>Konfidence in it</Company>
		<AssemblyCompany>Konfidence in it</AssemblyCompany>
    
		<LangVersion>latest</LangVersion>

		<ComVisible>false</ComVisible>
		<CLSCompliant>true</CLSCompliant>
		<AssemblyCulture>en</AssemblyCulture>

		<Product>iDealAdvancedConnector</Product>

		<PackageTags>iDealAdvancedConnector</PackageTags>
		<Description>Opensource tools and API to connect webshops and merchants to ING using iDeal</Description>

		<PackageOutputPath>$(SolutionDir)nuget</PackageOutputPath>

		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/a3helmich/ing-ideal-connectors-net</PackageProjectUrl>

		<PackageReadmeFile>README.md</PackageReadmeFile>

		<RepositoryUrl>https://github.com/a3helmich/ing-ideal-connectors-net</RepositoryUrl>
		<RepositoryType>git</RepositoryType>

		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
	</PropertyGroup>
	<ItemGroup>
		<None Include="$(SolutionDir)README.md" Pack="true" PackagePath="\"/>
	</ItemGroup>
</Project>
